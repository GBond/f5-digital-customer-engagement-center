def header_name;
for(int i=0; i<params._source.httpRequest.headers.length; i++) {
    header_name = params._source.httpRequest.headers[i].name.toLowerCase();
    if (header_name == "cookie") {
        def m = /(?<=_imp_di_pc_=).*;/.matcher(params._source.httpRequest.headers[i].value);
        if (m.find()) { return m.group(0) }
        else { return "no device-id found" } 
    }
}
